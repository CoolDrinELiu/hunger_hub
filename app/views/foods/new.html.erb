<h3>New Food</h3>
<%= form_for @food do |f| %>
  <%= f.label :name, class: "col-sm-2 col-form-label"%>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :desc, "Desc", class: "col-sm-2 col-form-label"%>
  <%= f.text_field :desc, class: "form-control" %>
  <%= f.label :end_at, "End At", class: "col-sm-2 col-form-label"%>
  <%= f.datetime_field :end_at, class: "form-control w-25" %>
  <%= f.label :desc, "Logo", class: "col-sm-2 col-form-label"%>
  <br>
  <%= f.file_field :logo, class: "col-sm-2 col-form-label" %>
  <br>
  <%= f.submit "Create!", class: "btn btn-outline-primary" %>
<% end %>